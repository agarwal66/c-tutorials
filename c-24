#include <stdio.h>
void storeTable(int arr[][10], int n, int m, int number);
int main (){
  int tables[2][6];
  storeTable(tables , 0 , 10 ,2);
  storeTable(tables, 1,16,3);
  for (int i = 0; i<10; i++){
    printf("%d\t", tables[0][i]);
  }
  for(int i =0; i<10; i++){
    printf("%d",tables[1][i]);
  }
  return 0;
}
void storeTable(int arr[][10],int n,int m, int numbers){
  for (int i = 0;i<m;i++){
    arr[n][i] = numbers * (i+1);
  }
}



















