#include<stdio.h>
int main()
{
	char ch;
	
	printf("Enter character : ");
	scanf("%c", &ch);
	
	if(ch>='0' && ch<='9')
		printf("It's a digit \n");
	else
		printf("It's not a digit \n");
	return 0;
}

