#include<stdio.h>
# include <string.h>
int countVowel(char arr[]);
int main (){
  char str[] = "hello world";
  slice(str, 3,5);
}
int countVowels(char str[]){
  int count = 0
  for (int i=0; str[i] != '\0'; i++){
    if (str[i] == 'a'|| str[i]=='e'|| str[i]== 'i')||
      str[i] == 'o' ]] str[i] == 'u'}{
      count++;
      }
}
