#include <stdio.h>
// fibonacchi seq
int fib(int n);

int main(){
fib(12);
return 0;  
}
int fib (int n){
  if (n ==0|| n==1){
    if ( n==0){
      return 0;
      
    }
    if(n==1){
      return 1;
    }
  }
  int fibNm1 = fib(n-1);
  int fibNm2 = fib(n-2);
  int fibN = fibNm1 + fibNm2;
  printf("fib of n %d is %d\n", n , fibN);
  return fibN;
}
