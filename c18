#include<math.h>
float squareArea(float side);
float circleArea(float r);
float rectangleArea(float a ,float b);
int main(){
  //float a = 5.55;
  //float b = 6.78;
  float r;
  printf("enter a radius");
  scanf("%f", &r);
  printf("r is :%f", 3.14*r*r);
  return 0;
}
float squareArea(float side){
  return side*side;
}
float circleArea(float r){
  return 3.14*r *r;
}
float rectangleArea(float a , float b){
  return a * b;
}

